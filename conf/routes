# Routes
# This file defines all application routes (Higher priority routes first)

# route that derects to the HomeController
GET     /                           controllers.HomeController.index
# route that derects to the GameController
GET     /game                         controllers.GameController.index
# route that derects to the SignupController
GET     /signup                  controllers.SignupController.index
# route that derects to the LoginController
GET     /login                  controllers.LoginController.index
# route that derects to the StatController
GET     /statistics                   controllers.StatController.index
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
